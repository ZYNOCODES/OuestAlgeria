                                            <% place?.historique?.img?.map((img) => { %>
                                                <div class="gallery-item">
                                                    <a href="/images/<%= img?.img %>" data-lightbox="mygallery" data-title="<%= img?.description %>">
                                                      <img src="/images/<%= img?.img  %>" alt="<%= img?.img  %>" />
                                                    </a>
                                                    <h3><%= img?.description %></h3>
                                                </div>                                            <% }); %>
                                        <% }%>
                                    </div>
                                </div>
                            </div>
                        <% } %>
